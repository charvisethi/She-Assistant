                    cout<<"https://docs.google.com/document/d/1locH8xI-nI9uGtcMsOO50FYqpbS4AwYuG3fVfemQ7dI/edit"<<endl;
                }
                s[74]=strcmp(size,"XL");
                t[74]=strcmp(color,"Red");
                if((s[74]==1) && (price<=2000 && price>1000) && (t[74]==1)){
                    cout<<"Hyperlinks of top options"<<endl;
